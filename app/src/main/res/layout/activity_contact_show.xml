<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="store.chinaotec.com.medicalcare.activity.ContactShowActivity">

    <LinearLayout
        android:id="@+id/linear_contact_show"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorTooBar"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/contact_show_back"
            android:layout_width="@dimen/px30"
            android:layout_height="@dimen/px28"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/px5"
            android:src="@mipmap/back_top_finsh" />

        <TextView
            android:id="@+id/one_call_titile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/px130"
            android:layout_toRightOf="@+id/contact_show_back"
            android:text="家人"
            android:textColor="@android:color/white"
            android:textSize="@dimen/px20" />

        <Button
            android:id="@+id/edit_contact_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/px90"
            android:background="@android:color/transparent"
            android:text="@string/edit_title"
            android:textColor="@android:color/white"
            android:textSize="@dimen/px16" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px98"
        android:layout_marginTop="@dimen/px20"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/contacts_name"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />

        <TextView
            android:id="@+id/contact_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px21"
            android:text="张三"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />
    </LinearLayout>

    <View
        android:layout_width="@dimen/px250"
        android:layout_height="@dimen/px1"
        android:layout_marginLeft="@dimen/px77"
        android:layout_marginTop="@dimen/px10"
        android:background="@color/myDeviceTile" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px98"
        android:layout_marginTop="@dimen/px16"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/contacts_phone"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />

        <TextView
            android:id="@+id/contact_phone_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px21"
            android:text="13526749956"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />
    </LinearLayout>

    <View
        android:layout_width="@dimen/px250"
        android:layout_height="@dimen/px1"
        android:layout_marginLeft="@dimen/px77"
        android:layout_marginTop="@dimen/px10"
        android:background="@color/myDeviceTile" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px98"
        android:layout_marginTop="@dimen/px16"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/contacts_phone"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />

        <TextView
            android:id="@+id/contact_phone_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px21"
            android:text="13526749956"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />
    </LinearLayout>

    <View
        android:layout_width="@dimen/px250"
        android:layout_height="@dimen/px1"
        android:layout_marginLeft="@dimen/px77"
        android:layout_marginTop="@dimen/px10"
        android:background="@color/myDeviceTile" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px98"
        android:layout_marginTop="@dimen/px16"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/contacts_phone"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />

        <TextView
            android:id="@+id/contact_phone_three"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px21"
            android:text="13526749956"
            android:textColor="@color/myDeviceTile"
            android:textSize="@dimen/px15" />
    </LinearLayout>

    <View
        android:layout_width="@dimen/px250"
        android:layout_height="@dimen/px1"
        android:layout_marginLeft="@dimen/px77"
        android:layout_marginTop="@dimen/px10"
        android:background="@color/myDeviceTile" />
</LinearLayout>
