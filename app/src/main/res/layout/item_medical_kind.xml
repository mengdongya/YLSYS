<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/px10"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/medical_kind_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:src="@mipmap/medical_sign_nurse" />

    <TextView
        android:id="@+id/medical_kind_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/px5"
        android:text="签约护士"
        android:textColor="@color/userPhone"
        android:textSize="@dimen/px9" />
</LinearLayout>